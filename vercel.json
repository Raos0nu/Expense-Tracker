{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist/expense-tracker-client",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
